#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_jest@29.7.0_typescript@5.9.2/node_modules/ts-jest/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_jest@29.7.0_typescript@5.9.2/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_jest@29.7.0_typescript@5.9.2/node_modules/ts-jest/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_jest@29.7.0_typescript@5.9.2/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
