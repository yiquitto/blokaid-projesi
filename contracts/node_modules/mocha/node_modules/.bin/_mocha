#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/mocha@9.2.2/node_modules/mocha/bin/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/mocha@9.2.2/node_modules/mocha/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/mocha@9.2.2/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/mocha@9.2.2/node_modules/mocha/bin/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/mocha@9.2.2/node_modules/mocha/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/mocha@9.2.2/node_modules:/mnt/c/Users/Yiğit/Desktop/blokaid-projesi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/_mocha" "$@"
else
  exec node  "$basedir/../../bin/_mocha" "$@"
fi
